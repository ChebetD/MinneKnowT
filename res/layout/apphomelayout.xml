<?xml version="1.0" encoding="utf-8"?>

<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:gravity="center_vertical"
    >
 
 <!--  android:background="#A38552"-->
    
    <TableRow
    	android:id="@+id/tableRow1"
    	android:layout_height="wrap_content"
    	android:layout_width="match_parent"
    	android:gravity="center"
    	>
	       
    	 <ImageButton 
	      	android:id="@+id/takeQuizButton"
	    	android:background="@null"   
	    	android:layout_width="wrap_content"
	    	android:layout_height="wrap_content"	 
	    	android:src="@drawable/takequiz"  
	    	android:onClick="showQuizActivity"/>  
 
    	 <ImageButton 
	      	android:id="@+id/roadSignButton"
	    	android:background="@null"   	 
	    	android:src="@drawable/roadsignicon"  
	    	android:onClick="showRoadSignActivity"/>  
	    	    	    	 
    </TableRow>

	<TableRow
    	android:id="@+id/tableRow2"
    	android:layout_height="wrap_content"
    	android:layout_width="wrap_content"
    	android:gravity="center"> 	
	
    	 <ImageButton 
	      	android:id="@+id/quizStatButton"
	    	android:background="@null"   	 
	    	android:src="@drawable/quizstaticon"  
	    	android:onClick="showQuizHistory"/>  	

<!-- 	    	
    	 <Button 
	      	android:id="@+id/quizReviewButton"
	      	android:text="quizReview"
	      	android:layout_width="wrap_content"
	      	android:layout_height="wrap_content"
	      	
	    	android:background="@drawable/quiz_question_text_border"   	   
	    	android:onClick="showReviewActivity"/>  
	  -->   		    	
	</TableRow>
	                       
</TableLayout>
